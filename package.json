{
  "name": "md-to-telegraph-action",
  "version": "1.1.1",
  "description": "GitHub Action to convert markdown files to Telegraph pages",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "package": "ncc build src/index.ts -o dist --source-map --license licenses.txt",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "prepare-release": "yarn build && yarn package && git add dist/",
    "release": "yarn prepare-release && yarn test",
    "check-dist": "yarn package && git diff --exit-code dist/"
  },
  "packageManager": "yarn@1.22.19",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cyanxiao/md-to-telegraph-action.git"
  },
  "keywords": [
    "github-action",
    "markdown",
    "telegraph",
    "documentation"
  ],
  "author": "Cyan Xiao",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "axios": "^1.6.0",
    "glob": "^10.3.10",
    "jsdom": "^23.0.1",
    "marked": "^9.1.6"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/marked": "^5.0.0",
    "@types/nock": "^11.1.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "jsdom": "^23.0.1",
    "nock": "^14.0.10",
    "prettier": "^3.1.0",
    "ts-jest": "^29.4.1",
    "typescript": "^5.3.0"
  }
}
